    "sinon": "^7.2.7",
    "spectron": "^5.0.0",
    "style-loader": "^0.23.1",
    "stylelint": "^9.10.1",
    "stylelint-config-standard": "^18.2.0",
    "url-loader": "^1.1.2",
    "webpack": "^4.29.6",
