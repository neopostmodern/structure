    "express": "^4.16.4",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "fbjs-scripts": "^1.1.0",
    "file-loader": "^3.0.1",
    "flow-bin": "^0.95.1",
    "flow-runtime": "^0.17.0",
    "flow-typed": "^2.5.1",
    "html-webpack-plugin": "^3.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.5.0",
    "jsdom": "^14.0.0",
    "mini-css-extract-plugin": "^0.5.0",
