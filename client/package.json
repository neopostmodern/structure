    "electron-devtools-installer": "^4.0.0",
    "electron-notarize": "^1.2.2",
    "electron-rebuild": "^3.2.9",
    "esbuild": "^0.25.9",
    "esbuild-register": "^3.6.0",
    "eslint": "^9.34.0",
