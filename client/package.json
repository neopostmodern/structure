{
  "name": "@structure/client",
  "productName": "Structure",
  "description": "Frontend (desktop and web app) for the Structure bookmarking software",
  "version": "0.24.1",
  "main": ".vite/build/main.js",
  "directories": {
    "lib": "lib",
    "test": "__tests__"
  },
  "files": [
    "lib"
  ],
  "scripts": {
    "build:web": "vite --config vite.web.config.ts build",
    "lint": "eslint --ext .ts,.tsx .",
    "generate-graphql-types": "graphql-codegen --config codegen.yml",
    "package": "electron-forge make",
    "package-publish": "electron-forge publish",
    "start": "electron-forge start",
    "start:web": "vite --config vite.web.config.ts",
    "prepare": "cd .. && husky install client/.husky"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "cross-env NODE_ENV=development eslint --cache"
    ],
    "*.json,.{eslintrc,prettierrc}": [
      "prettier --ignore-path .eslintignore --parser json --write"
    ],
    "*.{css,scss}": [
      "prettier --ignore-path .eslintignore --single-quote --write"
    ],
    "*.{html,md,yml}": [
      "prettier --ignore-path .eslintignore --single-quote --write"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/neopostmodern/structure.git"
  },
  "author": {
    "name": "neopostmodern",
    "email": "structure@neopostmodern.com",
    "url": "https://github.com/neopostmodern"
  },
  "license": "AGPL-3.0",
  "bugs": {
    "url": "https://github.com/neopostmodern/structure/issues"
  },
  "keywords": [
    "bookmarking",
    "structure",
    "electron",
    "react",
    "typescript"
  ],
  "homepage": "https://structure.love",
  "devDependencies": {
    "@electron-forge/cli": "^7.9.0",
    "@electron-forge/maker-deb": "^7.9.0",
    "@electron-forge/maker-rpm": "^7.9.0",
    "@electron-forge/maker-squirrel": "^7.9.0",
    "@electron-forge/maker-zip": "^7.9.0",
    "@electron-forge/plugin-auto-unpack-natives": "^7.9.0",
    "@electron-forge/plugin-fuses": "^7.9.0",
    "@electron-forge/plugin-vite": "^7.9.0",
    "@electron-forge/publisher-github": "^7.9.0",
    "@electron/fuses": "^1.8.0",
    "@graphql-codegen/cli": "5.0.7",
    "@graphql-codegen/fragment-matcher": "5.1.0",
    "@graphql-codegen/typescript": "4.1.6",
    "@graphql-codegen/typescript-operations": "4.6.1",
    "@graphql-codegen/typescript-react-apollo": "4.3.3",
    "@pmmmwh/react-refresh-webpack-plugin": "0.6.1",
    "@reforged/maker-appimage": "^5.0.0",
    "@simbathesailor/use-what-changed": "^2.0.0",
    "@types/lodash": "^4.17.20",
    "@types/mousetrap": "^1.6.15",
    "@types/node": "24.3.0",
    "@types/react": "^19.1.12",
    "@types/react-dom": "^19.1.9",
    "@types/react-test-renderer": "^19.1.0",
    "@types/redux-logger": "^3.0.13",
    "@types/styled-components": "^5.1.34",
    "@typescript-eslint/eslint-plugin": "^8.41.0",
    "@typescript-eslint/parser": "^8.41.0",
    "@vitejs/plugin-react": "^5.0.2",
    "browserslist": "^4.25.3",
    "concurrently": "^9.2.1",
    "copy-webpack-plugin": "^13.0.1",
    "cross-env": "^10.0.0",
    "css-loader": "^7.1.2",
