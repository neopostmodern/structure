    "electron-squirrel-startup": "^1.0.1",
    "graphql": "^16.11.0",
    "graphql-tag": "^2.12.6",
    "history": "^5.3.0",
    "localforage": "^1.10.0",
    "lodash": "^4.17.21",
    "match-sorter": "^8.1.0",
    "mousetrap": "^1.6.5",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-hook-form": "^7.62.0",
    "react-intersection-observer": "^9.16.0",
    "react-markdown": "^10.1.0",
    "react-redux": "^9.2.0",
    "react-router-dom": "^7.8.2",
    "react-timeago": "^8.3.0",
    "redux": "^5.0.1",
    "redux-act": "^1.8.0",
    "redux-first-history": "^5.2.0",
    "redux-logger": "^3.0.6",
    "rehype-raw": "^7.0.0",
    "rehype-sanitize": "^6.0.0",
    "remark-breaks": "^4.0.0",
    "remark-gfm": "^4.0.1",
    "styled-components": "^6.1.19",
    "update-electron-app": "^3.1.1"
  },
  "prettier": {
    "overrides": [
      {
        "files": [
          ".prettierrc",
          ".eslintrc"
        ],
        "options": {
          "parser": "json"
        }
      }
    ],
    "singleQuote": true
  },
  "publishConfig": {
    "access": "public"
  },
  "volta": {
    "extends": "../package.json"
  }
}
